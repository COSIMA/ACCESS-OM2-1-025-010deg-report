	Journal = {Deep-Sea Research. Part 2: Topical Studies in Oceanography},
	Keywords = {Antarctic Sea Ice Kinematics Dynamics Drifting Buoys},
	Note = {ISSN 0967-0645},
	Number = {9-10},
	Pages = {1158-1171},
	Title = {Physical attributes of sea-ice kinematics during spring 2007 off {East Antarctica}},
